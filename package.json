{
  "name": "boss-lib",
  "version": "0.1.0",
  "private": false,
  "main": "lib/boss-lib.common.js",
  "style": "lib/theme-chalk/index.css",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "bootstrap": "yarn || npm i",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:style": "gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "lib": "vue-cli-service build --target lib --name boss-lib --dest lib packages/index.js",
    "dist": "npm run clean && npm run common && npm run build:style",
    "conf": "webpack --config build/webpack.conf.js",
    "common": "webpack --config build/webpack.common.js && npm run build:utils"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.4.5",
    "exceljs": "^4.2.1",
    "jsoneditor": "^9.2.0",
    "sortablejs": "^1.13.0",
    "vue": "^2.6.11",
    "vxe-table": "2.10.4",
    "vxe-table-plugin-element": "^1.10.3",
    "vxe-table-plugin-excel": "^1.6.0",
    "vxe-table-plugin-export-xlsx": "^2.1.2",
    "vxe-table-plugin-menus": "^1.5.3",
    "xe-clipboard": "^1.10.1",
    "xe-utils": "2.8.3",
    "xlsx": "0.16.8",
    "vue-quill-editor": "^3.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "cross-env": "^3.1.3",
    "cp-cli": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^1.0.1",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.14.0",
    "webpack-cli": "^4.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}

{
  "name": "boss-lib",
  "version": "0.1.0",
  "private": false,
  "main": "lib/boss-lib.common.js",
  "style": "lib/theme-chalk/index.css",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "doc": "vue-styleguidist server",
    "doc:build": "vue-styleguidist build",
    "bootstrap": "yarn || npm i",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:style": "gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "lib": "vue-cli-service build --target lib --name boss-lib --dest lib packages/index.js",
    "conf": "webpack --config build/webpack.conf.js",
    "common": "webpack --config build/webpack.common.js && npm run build:utils",
    "comp": "webpack --config build/webpack.component.js",
    "dist": "npm run clean && npm run common && npm run comp && npm run build:utils && npm run build:style"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.4.5",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.5",
    "jsoneditor": "^9.2.0",
    "sortablejs": "^1.13.0",
    "vue": "^2.6.11",
    "vue-quill-editor": "^3.0.6",
    "vue-splitpane": "^1.0.6",
    "vxe-table": "2.10.4",
    "vxe-table-plugin-element": "^1.10.3",
    "vxe-table-plugin-excel": "^1.6.0",
    "vxe-table-plugin-export-xlsx": "^2.1.2",
    "vxe-table-plugin-menus": "^1.5.3",
    "xe-clipboard": "^1.10.1",
    "xe-utils": "2.8.3",
    "xlsx": "0.16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-stage-2": "^7.8.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^3.2.4",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "install": "^0.13.0",
    "node-sass": "^4.14.1",
    "npm": "^7.6.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^1.0.1",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-loader": "^15.9.6",
    "vue-splitpane": "^1.0.6",
    "vue-styleguidist": "^4.35.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2",
    "xlsx": "^0.16.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}

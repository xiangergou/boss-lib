{
  "name": "boss-lib",
  "version": "0.1.0",
  "private": false,
  "main": "lib/boss-lib.umd.min.js",
  "style": "lib/theme-chalk/index.css",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:style": "gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "lib": "vue-cli-service build --target lib --name boss-lib --dest lib packages/index.js",
    "dist": "npm run lib && npm run build:style"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.4.5",
    "vue": "^2.6.11",
    "vxe-table": "^3.1.8",
    "xe-utils": "^3.1.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "cp-cli": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^10.1.1",
    "url-loader": "^1.0.1",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.14.0",
    "webpack-cli": "^4.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
